library ieee;
use ieee.std_logic_1164.all
 
entity sumador is
beign(
     a:in std_logic,
	  b:in std_logic,
	  cent:in std_logic,
	  cout: out std_logic,
	  suma: out std_logic
)end entity;

architecture sum of sumador
    signal same,act: std_logic;
begin 
    same<=a and b;
	 act<=a  or b;
    cout<= act and cent;
	 suma<= cent xor act xor same;
end sum; 