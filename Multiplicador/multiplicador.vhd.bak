library ieee;
library work;
use ieee.std_logic_1164.all;
use ieee.numeric_std.all;
use work.all;

entity multiplicador is 
	port (
		a: in std_logic_vector(0 TO 3),
		b: in std_logic_vector(0 TO 2)
	);
end multiplicador;
	
architecture multiplicacion of multiplicador is
	signal cin, cout: std_logic;
	signal s : std_logic_vector(0 TO 6);
	signal d: std_logic_vector(0 TO 11);
	
	begin 
	      d(0) <= a(0) and b(0);
			d(1) <= a(1) and b(0);
			d(2) <= a(2) and b(0);
			d(3) <= a(3) and b(0);
			
			
			d(4) <= a(0) and b(1);
			d(5) <= a(1) and b(1);
			d(6) <= a(2) and b(1);
			d(7) <= a(3) and b(1);
			
					
			d(8) <= a(0) and b(2);
			d(9) <= a(1) and b(2);
			d(10) <= a(2) and b(2);
			d(11) <= a(3) and b(2);
			
			
end multiplicacion;